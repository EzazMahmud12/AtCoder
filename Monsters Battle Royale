#include<bits/stdc++.h>
using namespace std;
#define e4 ios_base::sync_with_stdio(false); cin.tie(NULL)
#define endl '\n'
#define ll  long long
#define test_case int tc;cin>>tc;while(tc--)


int main() {

	
	   e4;

	   int i,n;

	   cin>>n;

	   vector<int> v(n);

	   for(i=0; i<n; i++) cin>>v[i];

	   int gcd=v[0];

	   for(i=1; i<n; i++){
	   	gcd=__gcd(gcd,v[i]);
	   }	

	   cout<<gcd;
	    
}

	
